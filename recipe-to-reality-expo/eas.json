{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_REVENUECAT_IOS_KEY": "",
        "EXPO_PUBLIC_REVENUECAT_ANDROID_KEY": ""
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_REVENUECAT_IOS_KEY": "",
        "EXPO_PUBLIC_REVENUECAT_ANDROID_KEY": ""
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_REVENUECAT_IOS_KEY": "",
        "EXPO_PUBLIC_REVENUECAT_ANDROID_KEY": ""
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "",
        "ascAppId": "",
        "appleTeamId": ""
      },
      "android": {
        "serviceAccountKeyPath": "",
        "track": "internal"
      }
    }
  }
}
